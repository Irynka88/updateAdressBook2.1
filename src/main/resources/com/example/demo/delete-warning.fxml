<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.DeleteWarningController" prefHeight="150.0" prefWidth="300.0" stylesheets="@viewEditor.css">
    <children>
        <Label text="Попередження: видалення може призвести до зміни порядку осіб в адресній книзі." wrapText="true" />
        <HBox spacing="10.0">
            <Button fx:id="confirmButton" text="ОК" onAction="#confirmDelete"/>
            <Button fx:id="cancelButton" text="Відміна" onAction="#cancel"/>
        </HBox>
    </children>
</VBox>
