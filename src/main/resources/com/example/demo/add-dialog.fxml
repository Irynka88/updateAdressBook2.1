<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.AddController" stylesheets="@viewEditor.css">
    <children>
        <TextField fx:id="nameField" layoutX="14.0" layoutY="14.0" prefHeight="30.0" prefWidth="250.0" promptText="ПІП"/>
        <TextField fx:id="phoneField" layoutX="14.0" layoutY="60.0" prefHeight="30.0" prefWidth="250.0" promptText="Телефон"/>
        <Button fx:id="saveButton" layoutX="180.0" layoutY="110.0" mnemonicParsing="false" text="Зберегти" onAction="#saveContact"/>
        <Button layoutX="80.0" layoutY="110.0" mnemonicParsing="false" text="Відміна" onAction="#cancel"/>
    </children>
</AnchorPane>
